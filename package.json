{
  "name": "easy-location",
  "version": "1.0.0",
  "description": "Easier URL manipulation using window.history",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.{j,t}s?(x)\"",
    "pretty": "pretty-quick",
    "test": "jest"
  },
  "keywords": [
    "browser",
    "url",
    "location",
    "history",
    "routing"
  ],
  "author": "Alexey Komarov <alex7kom@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@alex7kom/sayonara-config": "0.7.0",
    "husky": "^4.2.1"
  },
  "dependencies": {
    "decode-uri-component": "^0.2.0",
    "object-assign": "^4.1.0",
    "query-string": "^5.0.0",
    "strict-uri-encode": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alex7Kom/easy-location.git"
  },
  "eslintConfig": {
    "extends": "./node_modules/@alex7kom/sayonara-config/src/configs/eslint-browser.js",
    "overrides": [
      {
        "files": [
          "*.?(test|spec).{j,t}s?(x)"
        ],
        "env": {
          "jest": true,
          "node": true,
          "es6": true
        },
        "parserOptions": {
          "ecmaVersion": 2018
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "sayonaraConfig": {
    "sets": [
      "browser-library",
      "npm-package"
    ]
  }
}
